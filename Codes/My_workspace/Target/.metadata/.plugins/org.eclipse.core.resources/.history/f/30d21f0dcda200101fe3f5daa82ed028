/* #############################    INCLUDING HEADER FILES ##########################################*/
#include "stm32_GPIO.h"

/* #############################    INIT CONSTANTS FOR THE REGISTERS ################################*/

#define SET_BIT	01

const MODE_REG_TYPE MODE_REG_INIT_CONFIG = {.bits = {.MODER0 = SET_BIT}};


/* #############################    FILLING THE INITIAL CONFIGURATIONS MEMMAP FOR GPIOA (SW)  #######*/

GPIO_TypeDef GPIOA_init_config = {.MODER = MODE_REG_INIT_CONFIG.all};
