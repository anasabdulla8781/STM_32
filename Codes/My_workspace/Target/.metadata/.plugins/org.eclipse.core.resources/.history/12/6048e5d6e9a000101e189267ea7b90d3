#include "stm32_basic_timer.h"


CR1_bits CR1_init_config = {	.CEN 	= 1,
						.ARPE 	= 1,
						.OPM	= 0};

BASIC_TIM_init_config TIM6_INIT_CFG = {CR1_init_config};

void init_config_tim6(BASIC_TIM_init_config* cfg)
{
	TIM6->CR1 = cfg->CR1;
}
