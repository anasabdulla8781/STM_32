#include "stm32_basic_timer.h"


CR1_bits CR1_init_config = {bits.CEN = 1, bits.ARPE = 1, bits.OPM	= 0};

BASIC_TIM TIM6_INIT_CFG = {CR1_init_config};

void init_config_tim6(BASIC_TIM* cfg)
{
	TIM6->CR1 = cfg->CR1;
}
