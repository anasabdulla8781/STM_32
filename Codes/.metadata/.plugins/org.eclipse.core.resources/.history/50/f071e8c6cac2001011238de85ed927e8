#include <stdint.h>
#include <app_main.h>
#include <STM32_GPT.h>
#include <STM32_RCC.h>
#include <STM32_GPIO.h>
#include <STM32_NVIC.h>

#if !defined(__SOFT_FP__) && defined(__ARM_FP)
  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif

void service_init()
{
	rcc_init();
	timer4_init();
	nvic_init(TIM4_ISR_NUMBER);
	pin_init(GREEN_LED_PIN,PIN_ALTERNATE_FUNCTION);
}

void app_init()
{
	pin_init(GREEN_LED_PIN,PIN_OUTPUT);
	pin_init(BLUE_LED_PIN,PIN_OUTPUT);
	pin_init(RED_LED_PIN,PIN_OUTPUT);
	pin_init(ORANGE_LED_PIN,PIN_OUTPUT);
	timer4_set_generic(16000,999)
}


int main(void)
{
	service_init();
	app_init();
	timer2_set(TIMER_CLK_ARR_1S , TIMER_CLK_PRESCALAR);


	while (1)
	{
		;
	}
}
